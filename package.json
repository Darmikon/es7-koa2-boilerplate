{
  "name": "es7-koa2-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Damikon",
  "license": "MIT",
  "keywords": [
    "es6",
    "es7",
    "node",
    "nodejs",
    "koajs",
    "koa",
    "koa2",
    "boilerplate",
    "starter",
    "javascript"
  ],
  "scripts": {
    "start": "npm-run-all --parallel inspector debug",
    "start:brk": "npm-run-all --parallel inspector debug:brk",
    "inspector": "node-inspector",
    "debug": "cross-env NODE_ENV=development nodemon --debug ./src",
    "debug:brk": "cross-env NODE_ENV=development nodemon --debug-brk ./src",
    "lint": "eslint ./src",
    "rd": "rimraf dist",
    "md": "mkdir dist",
    "compile": "cross-env NODE_ENV=production babel ./src --out-dir ./dist --source-maps inline --ignore node_modules/** ",
    "prebuild": "run-s rd",
    "build": "run-s -n md compile copy:views",
    "copy:views": "ncp src/views dist/views",
    "node:run:build": "node --debug ./dist",
    "build:inspect": "cross-env NODE_ENV=production npm-run-all --parallel inspector node:run:build"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "ncp": "^2.0.0",
    "node-inspector": "^1.1.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "swagger-jsdoc": "^1.9.6",
    "swagger-ui-koa": "^0.0.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "boom": "^5.1.0",
    "chalk": "^1.1.3",
    "hbs": "^4.0.1",
    "kcors": "2",
    "koa": "^2.2.0",
    "koa-better-body": "^3.0.2",
    "koa-body": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.0.0",
    "koa-compress": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-mount": "^3.0.0",
    "koa-qs": "^2.0.0",
    "koa-router": "^7.2.0",
    "koa-static": "^3.0.0",
    "koa-views": "^6.0.2",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "pretty-error": "^2.1.0",
    "winston": "^2.3.1"
  }
}
