{
  "name": "es7-koa2-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Damikon",
  "license": "MIT",
  "keywords": [
    "es6",
    "es7",
    "node",
    "nodejs",
    "koajs",
    "koa",
    "koa2",
    "boilerplate",
    "starter",
    "javascript"
  ],
  "scripts": {
    "build": "run-s -n lint md compile copy:views",
    "compile": "cross-env NODE_ENV=production babel ./src --out-dir ./dist --source-maps --ignore node_modules/** ",
    "copy:views": "ncp src/views dist/views",
    "debug": "cross-env NODE_ENV=development nodemon src/index.js --inspect --exec babel-node",
    "debug:brk": "cross-env NODE_ENV=development nodemon src/index.js --inspect-brk --exec babel-node",
    "lint": "eslint ./src",
    "md": "mkdir dist",
    "prebuild": "run-s rd",
    "start": "run-s debug",
    "serve": "cross-env NODE_ENV=production node dist/index.js",
    "serve:debug": "cross-env NODE_ENV=production node --inspect dist/index.js",
    "TODO#############test": "mocha --compilers js:babel-register",
    "rd": "rimraf dist"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "ncp": "^2.0.0",
    "nodemon": "^1.17.2",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "boom": "^7.2.0",
    "chalk": "^2.3.2",
    "hbs": "^4.0.1",
    "kcors": "2",
    "koa": "^2.5.0",
    "koa-better-body": "^3.0.4",
    "koa-body": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.0.0",
    "koa-compress": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-mount": "^3.0.0",
    "koa-qs": "^2.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "koa-views": "^6.1.4",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "pretty-error": "^2.1.1",
    "winston": "^2.4.1"
  }
}
